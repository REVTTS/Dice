    "./src/": "./src/",
    "./types/": "./types/",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "chevrotain": "^10.3.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "rollup": "^2.78.1",
    "rollup-plugin-istanbul": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2"
  }

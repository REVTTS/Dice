{
  "name": "@revtts/dice",
  "version": "0.0.7",
  "repository": "https://github.com/REVTTS/Dice.git",
  "author": "Ronald Zielaznicki",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "keywords": [
    "dice",
    "revtts",
    "roleplay",
    "role",
    "rpg"
  ],
  "types:": "./types/index.d.ts",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "lint": "eslint src/**",
    "setup": "husky install",
    "prepack": "yarn build",
    "test": "mocha 'src/**/*.test.js'"
  },
  "main": "./dist/Dice.mjs",
  "module": "./lib/Dice.mjs",
